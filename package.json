{
  "name": "rouh",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "db:migrate": "pnpm --filter @rouh/api run db:migrate",
    "db:setup": "pnpm --filter @rouh/api run db:setup",
    "seed": "tsx infra/seed.ts",
    "e2e": "tsx infra/e2e.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.0",
    "@prisma/client": "^5.17.0",
    "eslint": "^9.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.2",
    "prisma": "^5.17.0",
    "tsx": "^4.16.2",
    "turbo": "^2.5.6"
  },
  "dependencies": {
    "axios": "^1.12.2"
  }
}
