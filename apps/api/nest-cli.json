{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": []
  },
  "watchOptions": {
    "watchAssets": true,
    "ignored": [
      "**/node_modules/**",
      "**/dist/**",
      "**/prisma/migrations/**",
      "**/*.spec.ts",
      "**/*.test.ts"
    ]
  }
}

